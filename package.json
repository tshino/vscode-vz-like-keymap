{
    "name": "vz-like-keymap",
    "displayName": "VZ Keymap",
    "description": "A keymap similar to VZ Editor",
    "version": "0.3.0",
    "publisher": "tshino",
    "engines": {
        "vscode": "^1.40.0"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "vz",
        "vzeditor"
    ],
    "homepage": "https://github.com/tshino/vscode-vz-like-keymap/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/tshino/vscode-vz-like-keymap.git"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "contributes": {
        "keybindings": [

            {
                "key": "ctrl+s",
                "command": "vz.cursorLeft",
                "when": "textInputFocus"
            }, {
                "key": "left",
                "command": "vz.cursorLeft",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+d",
                "command": "vz.cursorRight",
                "when": "textInputFocus"
            }, {
                "key": "right",
                "command": "vz.cursorRight",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+e",
                "command": "vz.cursorUp",
                "when": "textInputFocus"
            }, {
                "key": "up",
                "command": "vz.cursorUp",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+x",
                "command": "vz.cursorDown",
                "when": "textInputFocus"
            }, {
                "key": "down",
                "command": "vz.cursorDown",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+a",
                "command": "vz.cursorWordStartLeft",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+f",
                "command": "vz.cursorWordStartRight",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+r",
                "command": "vz.cursorPageUp",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+c",
                "command": "vz.cursorPageDown",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+w",
                "command": "scrollLineUp",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+z",
                "command": "scrollLineDown",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+s",
                "command": "vz.cursorLineStart",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q s",
                "command": "vz.cursorLineStart",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+d",
                "command": "vz.cursorLineEnd",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q d",
                "command": "vz.cursorLineEnd",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+r",
                "command": "vz.cursorTop",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q r",
                "command": "vz.cursorTop",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+c",
                "command": "vz.cursorBottom",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q c",
                "command": "vz.cursorBottom",
                "when": "textInputFocus"
            },

            {
                "key": "ctrl+b",
                "command": "vz.toggleSelection",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+k ctrl+b",
                "command": "vz.toggleBoxSelection",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+k b",
                "command": "vz.toggleBoxSelection",
                "when": "textInputFocus"
            }, {
                "key": "escape",
                "command": "vz.stopBoxSelection",
                "when": "editorHasMultipleSelections && textInputFocus"
            }, {
                "key": "shift+escape",
                "command": "vz.stopBoxSelection",
                "when": "editorHasMultipleSelections && textInputFocus"
            },

            {
                "key": "ctrl+q ctrl+j",
                "command": "workbench.action.gotoLine",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q j",
                "command": "workbench.action.gotoLine",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+k",
                "command": "editor.action.jumpToBracket",
                "when": "editorTextFocus"
            }, {
                "key": "ctrl+q k",
                "command": "editor.action.jumpToBracket",
                "when": "editorTextFocus"
            },

            {
                "key": "ctrl+h",
                "command": "vz.deleteLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "backspace",
                "command": "vz.deleteLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+g",
                "command": "vz.deleteRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "delete",
                "command": "vz.deleteRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q ctrl+h",
                "command": "vz.deleteWordLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q h",
                "command": "vz.deleteWordLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+t",
                "command": "vz.deleteWordRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+delete",
                "command": "vz.deleteWordRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q ctrl+t",
                "command": "vz.deleteAllLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q t",
                "command": "vz.deleteAllLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q ctrl+y",
                "command": "vz.deleteAllRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q y",
                "command": "vz.deleteAllRight",
                "when": "textInputFocus && !editorReadonly"
            },

            {
                "key": "ctrl+i",
                "command": "tab",
                "when": "editorTextFocus && !editorReadonly"
            }, {
                "key": "ctrl+m",
                "command": "type",
                "args": { "text": "\n" },
                "when": "editorTextFocus && !editorReadonly"
            }, {
                "key": "ctrl+n",
                "command": "editor.action.insertLineBefore",
                "when": "editorTextFocus && !editorReadonly"
            }, {
                "key": "ctrl+k ctrl+d",
                "command": "editor.action.copyLinesDownAction",
                "when": "editorTextFocus && !editorReadonly"
            }, {
                "key": "ctrl+k d",
                "command": "editor.action.copyLinesDownAction",
                "when": "editorTextFocus && !editorReadonly"
            },

            {
                "key": "ctrl+y",
                "command": "vz.clipboardCut",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+k ctrl+k",
                "command": "vz.clipboardCopy",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+k k",
                "command": "vz.clipboardCopy",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+j",
                "command": "vz.clipboardPaste",
                "when": "textInputFocus && !editorReadonly"
            },

            {
                "key": "ctrl+k ctrl+u",
                "command": "undo",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+k u",
                "command": "undo",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "alt+backspace",
                "command": "undo",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "shift+alt+backspace",
                "command": "redo",
                "when": "textInputFocus && !editorReadonly"
            },

            {
                "key": "ctrl+q ctrl+f",
                "command": "actions.find",
                "when": "editorTextFocus"
            }, {
                "key": "ctrl+q f",
                "command": "actions.find",
                "when": "editorTextFocus"
            }, {
                "key": "ctrl+l",
                "command": "actions.find",
                "when": "editorTextFocus"
            }, {
                "key": "ctrl+q ctrl+a",
                "command": "editor.action.startFindReplaceAction",
                "when": "editorTextFocus"
            }, {
                "key": "ctrl+q a",
                "command": "editor.action.startFindReplaceAction",
                "when": "editorTextFocus"
            },

            {
                "key": "ctrl+q ctrl+q",
                "command": "workbench.action.quickOpenView",
                "when": "textInputFocus"
            }
       ]
    }
}