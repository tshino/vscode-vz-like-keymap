{
    "name": "vz-like-keymap",
    "displayName": "Vz Keymap",
    "description": "A keymap similar to Vz Editor",
    "version": "0.7.0",
    "publisher": "tshino",
    "engines": {
        "vscode": "^1.41.0"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "vz",
        "vzeditor"
    ],
    "homepage": "https://github.com/tshino/vscode-vz-like-keymap/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/tshino/vscode-vz-like-keymap.git"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "contributes": {
        "keybindings": [

            {
                "key": "ctrl+s",
                "command": "",
                "when": "editorFocus"
            }, {
                "key": "ctrl+s",
                "command": "vz.cursorLeft",
                "when": "textInputFocus"
            }, {
                "key": "left",
                "command": "vz.cursorLeft",
                "when": "textInputFocus"
            }, {
                "key": "shift+left",
                "command": "vz.cursorLeftSelect",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+d",
                "command": "vz.cursorRight",
                "when": "textInputFocus"
            }, {
                "key": "right",
                "command": "vz.cursorRight",
                "when": "textInputFocus"
            }, {
                "key": "shift+right",
                "command": "vz.cursorRightSelect",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+e",
                "command": "vz.cursorUp",
                "when": "textInputFocus"
            }, {
                "key": "up",
                "command": "vz.cursorUp",
                "when": "textInputFocus"
            }, {
                "key": "shift+up",
                "command": "vz.cursorUpSelect",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+x",
                "command": "vz.cursorDown",
                "when": "textInputFocus"
            }, {
                "key": "down",
                "command": "vz.cursorDown",
                "when": "textInputFocus"
            }, {
                "key": "shift+down",
                "command": "vz.cursorDownSelect",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+a",
                "command": "vz.cursorWordStartLeft",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+f",
                "command": "vz.cursorWordStartRight",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+r",
                "command": "vz.cursorPageUp",
                "when": "textInputFocus"
            }, {
                "key": "pageup",
                "command": "vz.cursorPageUp",
                "when": "textInputFocus"
            }, {
                "key": "shift+pageup",
                "command": "vz.cursorPageUpSelect",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+c",
                "command": "vz.cursorPageDown",
                "when": "textInputFocus"
            }, {
                "key": "pagedown",
                "command": "vz.cursorPageDown",
                "when": "textInputFocus"
            }, {
                "key": "shift+pagedown",
                "command": "vz.cursorPageDownSelect",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+w",
                "command": "vz.scrollLineUp",
                "when": "editorFocus"
            }, {
                "key": "ctrl+w",
                "command": "vz.scrollLineUpUnselect",
                "when": "editorFocus && findWidgetVisible"
            }, {
                "key": "ctrl+z",
                "command": "vz.scrollLineDown",
                "when": "editorFocus"
            }, {
                "key": "ctrl+z",
                "command": "vz.scrollLineDownUnselect",
                "when": "editorFocus && findWidgetVisible"
            }, {
                "key": "ctrl+q ctrl+s",
                "command": "vz.cursorLineStart",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q s",
                "command": "vz.cursorLineStart",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+d",
                "command": "vz.cursorLineEnd",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q d",
                "command": "vz.cursorLineEnd",
                "when": "textInputFocus"
            }, {
                "key": "home",
                "command": "vz.cursorHome",
                "when": "textInputFocus"
            }, {
                "key": "shift+home",
                "command": "vz.cursorHomeSelect",
                "when": "textInputFocus"
            }, {
                "key": "end",
                "command": "vz.cursorEnd",
                "when": "textInputFocus"
            }, {
                "key": "shift+end",
                "command": "vz.cursorEndSelect",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+r",
                "command": "cursorTop",
                "when": "editorFocus"
            }, {
                "key": "ctrl+q ctrl+r",
                "command": "vz.cursorTop",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q r",
                "command": "cursorTop",
                "when": "editorFocus"
            }, {
                "key": "ctrl+q r",
                "command": "vz.cursorTop",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+c",
                "command": "cursorBottom",
                "when": "editorFocus"
            }, {
                "key": "ctrl+q ctrl+c",
                "command": "vz.cursorBottom",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q c",
                "command": "cursorBottom",
                "when": "editorFocus"
            }, {
                "key": "ctrl+q c",
                "command": "vz.cursorBottom",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+e",
                "command": "vz.cursorViewTop",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q e",
                "command": "vz.cursorViewTop",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+x",
                "command": "vz.cursorViewBottom",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q x",
                "command": "vz.cursorViewBottom",
                "when": "textInputFocus"
            },

            {
                "key": "ctrl+b",
                "command": "",
                "when": "editorFocus"
            }, {
                "key": "ctrl+b",
                "command": "vz.toggleSelection",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+k ctrl+b",
                "command": "vz.toggleBoxSelection",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+k b",
                "command": "vz.toggleBoxSelection",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+b",
                "command": "vz.reverseSelection",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q b",
                "command": "vz.reverseSelection",
                "when": "textInputFocus"
            }, {
                "key": "escape",
                "command": "vz.toggleSelection",
                "when": "textInputFocus && vz.inSelectionMode && !editorHasSelection && !suggestWidgetVisible && !accessibilityHelpWidgetVisible && !breakpointWidgetVisible && !findWidgetVisible"
            }, {
                "key": "shift+escape",
                "command": "vz.toggleSelection",
                "when": "textInputFocus && vz.inSelectionMode && !editorHasSelection && !suggestWidgetVisible && !accessibilityHelpWidgetVisible && !breakpointWidgetVisible && !findWidgetVisible"
            }, {
                "key": "escape",
                "command": "vz.stopBoxSelection",
                "when": "editorHasMultipleSelections && textInputFocus"
            }, {
                "key": "shift+escape",
                "command": "vz.stopBoxSelection",
                "when": "editorHasMultipleSelections && textInputFocus"
            },

            {
                "key": "ctrl+q ctrl+j",
                "command": "workbench.action.gotoLine",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q j",
                "command": "workbench.action.gotoLine",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+q ctrl+k",
                "command": "editor.action.jumpToBracket",
                "when": "editorTextFocus"
            }, {
                "key": "ctrl+q k",
                "command": "editor.action.jumpToBracket",
                "when": "editorTextFocus"
            }, {
                "key": "shift+f10",
                "command":  "vz.tagJump",
                "when": "editorTextFocus"
            },

            {
                "key": "ctrl+h",
                "command": "",
                "when": "editorFocus"
            }, {
                "key": "ctrl+h",
                "command": "vz.deleteLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "backspace",
                "command": "vz.deleteLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+g",
                "command": "",
                "when": "editorFocus"
            }, {
                "key": "ctrl+g",
                "command": "vz.deleteRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "delete",
                "command": "vz.deleteRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q ctrl+h",
                "command": "vz.deleteWordLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q h",
                "command": "vz.deleteWordLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+t",
                "command": "",
                "when": "editorFocus"
            }, {
                "key": "ctrl+t",
                "command": "vz.deleteWordRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+delete",
                "command": "vz.deleteWordRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q ctrl+t",
                "command": "vz.deleteAllLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q t",
                "command": "vz.deleteAllLeft",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q ctrl+y",
                "command": "vz.deleteAllRight",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+q y",
                "command": "vz.deleteAllRight",
                "when": "textInputFocus && !editorReadonly"
            },

            {
                "key": "ctrl+i",
                "command": "tab",
                "when": "editorTextFocus && !editorReadonly"
            }, {
                "key": "ctrl+m",
                "command": "",
                "when": "editorFocus"
            }, {
                "key": "ctrl+m",
                "command": "type",
                "args": { "text": "\n" },
                "when": "editorTextFocus && !editorReadonly"
            }, {
                "key": "ctrl+n",
                "command": "",
                "when": "editorFocus"
            }, {
                "key": "ctrl+n",
                "command": "editor.action.insertLineBefore",
                "when": "editorTextFocus && !editorReadonly"
            }, {
                "key": "ctrl+k ctrl+d",
                "command": "editor.action.copyLinesDownAction",
                "when": "editorTextFocus && !editorReadonly"
            }, {
                "key": "ctrl+k d",
                "command": "editor.action.copyLinesDownAction",
                "when": "editorTextFocus && !editorReadonly"
            },

            {
                "key": "ctrl+y",
                "command": "vz.clipboardCut",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+k ctrl+k",
                "command": "vz.clipboardCopy",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+k k",
                "command": "vz.clipboardCopy",
                "when": "textInputFocus"
            }, {
                "key": "ctrl+j",
                "command": "",
                "when": "editorFocus"
            }, {
                "key": "ctrl+j",
                "command": "vz.clipboardPaste",
                "when": "textInputFocus && !editorReadonly"
            },

            {
                "key": "ctrl+k ctrl+u",
                "command": "undo",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "ctrl+k u",
                "command": "undo",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "alt+backspace",
                "command": "undo",
                "when": "textInputFocus && !editorReadonly"
            }, {
                "key": "shift+alt+backspace",
                "command": "redo",
                "when": "textInputFocus && !editorReadonly"
            },

            {
                "key": "ctrl+q ctrl+f",
                "command": "vz.find",
                "when": "editorFocus"
            }, {
                "key": "ctrl+q f",
                "command": "vz.find",
                "when": "editorFocus"
            }, {
                "key": "ctrl+l",
                "command": "actions.find",
                "when": "editorTextFocus"
            }, {
                "key": "ctrl+q ctrl+a",
                "command": "editor.action.startFindReplaceAction",
                "when": "editorFocus"
            }, {
                "key": "ctrl+q a",
                "command": "editor.action.startFindReplaceAction",
                "when": "editorFocus"
            }, {
                "key": "ctrl+e",
                "command": "history.showPrevious",
                "when": "historyNavigationEnabled && historyNavigationWidget"
            }, {
                "key": "ctrl+x",
                "command": "history.showNext",
                "when": "historyNavigationEnabled && historyNavigationWidget"
            }, {
                "key": "pageup",
                "command": "editor.action.previousMatchFindAction",
                "when": "editorFocus && findWidgetVisible"
            }, {
                "key": "ctrl+r",
                "command": "editor.action.previousMatchFindAction",
                "when": "editorFocus && findWidgetVisible"
            }, {
                "key": "ctrl+m",
                "command": "editor.action.nextMatchFindAction",
                "when": "editorFocus && findInputFocussed"
            }, {
                "key": "pagedown",
                "command": "editor.action.nextMatchFindAction",
                "when": "editorFocus && findWidgetVisible"
            }, {
                "key": "ctrl+c",
                "command": "editor.action.nextMatchFindAction",
                "when": "editorFocus && findWidgetVisible"
            }, {
                "key": "ctrl+m",
                "command": "editor.action.replaceOne",
                "when": "editorFocus && findWidgetVisible && replaceInputFocussed"
            }, {
                "key": "ctrl+f",
                "command": "editor.action.startFindReplaceAction",
                "when": "editorFocus && findWidgetVisible && findInputFocussed"
            }, {
                "key": "ctrl+f",
                "command": "vz.find",
                "when": "editorFocus && findWidgetVisible && replaceInputFocussed"
            }, {
                "key": "ctrl+s",
                "command": "vz.closeFindWidget",
                "when": "editorFocus && findInputFocussed || editorFocus && replaceInputFocussed"
            }, {
                "key": "ctrl+d",
                "command": "vz.closeFindWidget",
                "when": "editorFocus && findInputFocussed || editorFocus && replaceInputFocussed"
            }, {
                "key": "ctrl+b",
                "command": "vz.closeFindWidget",
                "when": "editorFocus && findInputFocussed || editorFocus && replaceInputFocussed"
            },

            {
                "key": "ctrl+q ctrl+q",
                "command": "workbench.action.quickOpenView",
                "when": "editorFocus"
            }, {
                "key": "ctrl+alt+b",
                "command": "workbench.action.toggleSidebarVisibility",
                "when": ""
            }, {
                "key": "ctrl+alt+j",
                "command": "workbench.action.togglePanel",
                "when": ""
            }
        ]
    }
}